{
  "minify": true,
  "jsc": {
    "minify": {
      "compress": { "unused": true },
      "mangle": true
    },
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "react": {
        "runtime": "automatic",
        "throwIfNamespace": true,
        "development": true,
        "useBuiltins": false
      },
      "optimizer": {
        "globals": {
          "vars": {
            "__DEBUG__": "true"
          }
        }
      }
    },
    "target": "es5",
    "loose": false
  },
  "module": {
    "type": "commonjs"
  }
}